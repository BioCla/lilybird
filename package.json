{
  "name": "lilybird",
  "version": "0.1.1",
  "description": "A bun-first discord client written in TS",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/tsx/index.d.ts",
      "require": "./dist/tsx/index.js",
      "default": "./dist/tsx/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=21"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "bun run build && bun test.tsx",
    "lint": "eslint src/**/*.ts",
    "build": "rm -rf dist && tsc",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Didas-git/lilybird"
  },
  "keywords": [
    "discord",
    "wrapper",
    "client",
    "discord-client",
    "discord-wrapper",
    "bun",
    "bun-first"
  ],
  "author": "DidaS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Didas-git/lilybird/issues"
  },
  "homepage": "https://github.com/Didas-git/lilybird#readme",
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "bun-types": "^1.0.12",
    "eslint": "^8.54.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "typescript": "^5.2.2"
  }
}